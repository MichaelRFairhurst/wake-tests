module wktest;

import wunit.Asserts;

@TestClass
every CastTest is:

	@Test
	testUnsafeCastRunsCode(Asserts) {
		var Num = 0;
		(unsafe cast Num) (Num := 5);

		Asserts.that(Num)Equals(5);
	}
